<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>sunpack</AssemblyName>
    <PublishAOT>true</PublishAOT>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="TeuJson" Version="3.2.0" />
    <PackageReference Include="TeuJson.Generator" Version="3.2.0" />
    <PackageReference Include="NLua" Version="1.7.3" />
  </ItemGroup>

  <ItemGroup>
		<EmbeddedResource Include="init.lua">
			<LogicalName>init.lua</LogicalName>
		</EmbeddedResource>
  </ItemGroup>

</Project>
